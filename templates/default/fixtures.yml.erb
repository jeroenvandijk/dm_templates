<% 1.upto(number_of_entities) do |id| -%>
<%= "#{file_name}_#{id}" %>:
<% for attribute in attributes -%>
  <%= attribute.name %>: <%= attribute.default %>
<% end -%>
<% (has_and_belongs_to_many_association_names + has_many_association_names).each do |child| -%>
  <%= "#{child}: " + (id..number_of_entities).map{|x| "#{child.singularize}_#{x}" }.join(", ")%>
<% end -%>
<% belongs_to_association_names.each do |parent| -%>
  <%= "#{parent}: #{parent.singularize}_#{id}" %>
<% end -%>

<% end -%>