<div class="partial partial_for_<%= model.singular_name %> <%%= cycle('odd', 'even') %>">
<% model.attributes_for(:index).each do |attribute| -%>
	<div class="attribute_value format_<%= attribute.format %>">
		<%= attribute.display(:partial) %>
	</div>
<% end -%>

  <div class="link_to_edit"><%= model.link_to(:edit, :template => :partial) %></div>
  <div class="link_to_destroy"><%= model.link_to(:destroy, :template => :partial) %></div>
  <div class="link_to_index"><%= model.link_to(:index, :template => :partial) %></div>
</div>
